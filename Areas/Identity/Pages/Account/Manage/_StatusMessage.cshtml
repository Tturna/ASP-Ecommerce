@model string

@if (!String.IsNullOrEmpty(Model))
{
    var statusMessageClass = Model.StartsWith("Error") ? "c-primary" : "c-tertiary";
    <div class="@statusMessageClass" role="alert">
        @* <button type="button" class="btn btn-small btn-primary" data-bs-dismiss="alert">Close</button> *@
        @Model
    </div>
}
